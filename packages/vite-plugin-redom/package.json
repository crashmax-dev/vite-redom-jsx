{
  "name": "vite-plugin-redom",
  "version": "0.3.0",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./jsx": {
      "import": "./dist/jsx.js",
      "require": "./dist/jsx.cjs",
      "types": "./dist/jsx.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "redom-jsx": "workspace:^0.1.0"
  },
  "devDependencies": {
    "vite": "^3.0.9"
  },
  "peerDependencies": {
    "redom": ">=3.29.0",
    "redom-jsx": "workspace:^0.1.0"
  }
}
