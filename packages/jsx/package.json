{
  "name": "redom-jsx",
  "version": "0.0.0",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "build": "tsup src/index.ts --format esm,cjs --dts --minify"
  },
  "devDependencies": {
    "redom": "^3.29.0"
  },
  "peerDependencies": {
    "redom": ">=3.29.0"
  }
}
